<Window x:Class="ModuloControleEstoque.View.frmLogin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ModuloControleEstoque.View"
        mc:Ignorable="d"
        Title="Login" Height="300" Width="300" Loaded="Window_Loaded" Closing="Window_Closing">
    <Grid>
        <Label x:Name="lblUsuario" Content="Usuario" HorizontalAlignment="Left" Margin="23,32,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtUsuario" HorizontalAlignment="Left" Height="23" Margin="100,34,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblSenha" Content="Senha" HorizontalAlignment="Left" Margin="23,80,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.417,1.739"/>
        <TextBox x:Name="txtSenha" HorizontalAlignment="Left" Height="23" Margin="100,80,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnEntrar" Content="Entrar" HorizontalAlignment="Left" Margin="197,182,0,0" VerticalAlignment="Top" Width="75" Click="btnEntrar_Click"/>



    </Grid>
</Window>
